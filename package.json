{
  "name": "modelpad",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:vite": "vite --config vite.dev.config.ts && go run .",
    "dev:go": "go run main.go",
    "dev:db": "docker-compose up -d",
    "dev": "concurrently -n vite,go,db -c blue,green,magenta \"npm run dev:vite\" \"npm run dev:go\" \"npm run dev:db\"",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.19.2",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@floating-ui/dom": "^1.6.12",
    "@floating-ui/react": "^0.26.27",
    "@heroicons/react": "^2.1.5",
    "@lexical/react": "^0.19.0",
    "@types/filesystem": "^0.0.35",
    "classnames": "^2.5.1",
    "lexical": "^0.19.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-query": "^3.39.3",
    "supertokens-auth-react": "^0.48.0",
    "supertokens-web-js": "^0.14.0",
    "ulid": "^2.3.0",
    "use-debounce": "^10.0.4",
    "uuid": "^9.0.1",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.3",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.14",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  }
}
