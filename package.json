{
  "name": "modelpad",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:vite": "vite --config vite.dev.config.ts && go run .",
    "dev:go": "go run main.go",
    "dev:db": "docker-compose up -d",
    "dev": "concurrently -n vite,go,db -c blue,green,magenta \"yarn dev:vite\" \"yarn dev:go\" \"yarn dev:db\"",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.19.0",
    "@floating-ui/dom": "^1.6.3",
    "@floating-ui/react": "^0.26.9",
    "@heroicons/react": "^2.1.1",
    "@lexical/react": "^0.13.1",
    "@types/filesystem": "^0.0.35",
    "classnames": "^2.5.1",
    "lexical": "^0.13.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "supertokens-auth-react": "^0.40.0",
    "supertokens-web-js": "^0.10.1",
    "ulid": "^2.3.0",
    "use-debounce": "^10.0.0",
    "uuid": "^9.0.1",
    "zustand": "^4.5.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  }
}
